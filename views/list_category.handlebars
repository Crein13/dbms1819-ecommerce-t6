<div class="container">
	<div class="row">
		<h1>Category List</h1>
		{{#each rows}}		
		<div class="responsive">
			<div style="margin-top: 10pt; width: 420pt; margin-right: 18pt" class="card text-black card-body bg-info mb-3">
				<h3 class="card-title" readonly>{{this.category_name}}</h3>
			</div>
		</div>		
		{{/each}}
	</div>
	<div class="row">
		<div class="col-lg-12 text-center" style="margin-top: 25pt">
			<div class="text-center">
				<a href="/category/create"><button class="btn btn-primary btn-lg" title="Submit">ADD</button></a>
			</div>
		</div>
	</div>
</div>